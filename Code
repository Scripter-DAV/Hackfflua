gg.alert("ⓜⓐⓓⓔ ⓑⓨ ⒹⒶⓋ")

gg.alert("chịch free fire tới chết") 

gg.alert("bật chức năng có chữ safe = antiban")

function bodyHeadshot()
    gg.toast("BODY HEADSHOT ON")
    gg.setRanges(gg.REGION_ANONYMOUS)
    gg.searchNumber(";bone_Spine")
    gg.getResults(gg.getResultsCount())
    gg.editAll(";bone_Head1", gg.TYPE_WORD)
    gg.clearResults()
    gg.searchNumber("-0.04462028295", gg.TYPE_FLOAT)
    gg.getResults(gg.getResultsCount())
    gg.editAll("-0.03999999911", gg.TYPE_FLOAT)
    gg.clearResults()
end

function magicBullet()
    gg.toast("MAGIC BULLET 400% ON")
    gg.setRanges(32)
    gg.searchNumber("h23AAA6B8460ACD70", 1)
    gg.getResults(gg.getResultsCount())
    gg.editAll("h23AAA6B8B2F71FA4", 1)
    gg.clearResults()
end

function awmAimbot()
    gg.toast("AWM AIMBOT ON")
    gg.setRanges(gg.REGION_ANONYMOUS)
    gg.searchNumber("8.40779079e-45F;0D;0F;30F;50F;0.5F::33", gg.TYPE_DWORD)
    gg.refineNumber("0", gg.TYPE_DWORD)
    gg.getResults(1)
    gg.editAll("-1", gg.TYPE_DWORD)
    gg.clearResults()
end

function awmFastSwitch()
    gg.toast("AWM FAST SWITCH ON")
    gg.setRanges(gg.REGION_ANONYMOUS | gg.REGION_CODE_APP)
    gg.searchNumber("h 5C 43 00 00 90 42 00 00 B4 42", gg.TYPE_BYTE)
    gg.getResults(100)
    gg.editAll("h 5c 43 00 00 8c 42 00 00 b4 42", gg.TYPE_BYTE)
    gg.clearResults()
end

function whiteBody()
    gg.toast("ANTINA HAND ON")
    gg.setRanges(gg.REGION_ANONYMOUS)
    gg.searchNumber("-0.02980032004;1;0.48141112924::9", gg.TYPE_FLOAT)
    gg.refineNumber("1", gg.TYPE_FLOAT)
    gg.getResults(gg.getResultsCount())
    gg.editAll("3000", gg.TYPE_FLOAT)
    gg.clearResults()
end

function antibanHead()
    gg.toast("ANTINA HEAD ON")
    gg.setRanges(gg.REGION_ANONYMOUS)
    gg.searchNumber("5.9762459e-7;1::5 ", gg.TYPE_FLOAT)
    gg.refineNumber("1", gg.TYPE_FLOAT)
    gg.getResults(gg.getResultsCount())
    gg.editAll("3000", gg.TYPE_FLOAT)
    gg.clearResults()
end

function antinaHand()
    gg.toast("NIGHT MODE ON")
    gg.setRanges(gg.REGION_CODE_APP)
    gg.searchNumber("0.99;0.57735025883;1e-6::13", gg.TYPE_FLOAT)
    gg.refineNumber("1e-6", gg.TYPE_FLOAT)
    gg.getResults(gg.getResultsCount())
    gg.editAll("-1", gg.TYPE_FLOAT)
    gg.clearResults()
end

function BYPASSPROV1()
  gg.clearResults()
  gg.setRanges(gg.REGION_C_ALLOC)
  gg.searchNumber("1954047316;1970037078", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
  gg.searchNumber("1954047316;1970037078", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
  gg.getResults(10)
  gg.editAll("9", gg.TYPE_DWORD)
  gg.clearResults()
  gg.editAll(1, gg.TYPE_AUTO)
  gg.clearResults()
  gg.searchNumber("1", gg.TYPE_AUTO)
  gg.clearResults()
  gg.setRanges(gg.REGION_BAD)
  gg.clearResults()
  gg.searchNumber("2.718519e-43F;3.7615819e-37F;2.0F;0.00999999978F::200", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
  gg.clearResults()
  gg.searchNumber("2.718519e-43F;3.7615819e-37F;2.0F;0.00999999978F::200", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
  gg.clearResults()
  gg.searchNumber("2.718519e-43F;3.7615819e-37F;2.0F;0.00999999978F::200", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
  gg.clearResults()
  gg.searchNumber("2.718519e-43F;3.7615819e-37F;2.0F;0.00999999978F::200", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
  gg.clearResults()
  gg.searchNumber("2.718519e-43F;3.7615819e-37F;2.0F;0.00999999978F::200", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
  gg.clearResults()
  gg.toast("ʙʏᴘᴀss ᴠ𝟷 ON")
end

function BYPASSPROV2()
  gg.clearResults()
  gg.setRanges(gg.REGION_ANONYMOUS)
  gg.searchNumber("5001;1.1;1F::10", gg.TYPE_DOUBLE, false, gg.SIGN_EQUAL, 0, -1)
  gg.clearResults()
  gg.searchNumber("1.2F;1.8F:9::1", gg.TYPE_DOUBLE, false, gg.SIGN_EQUAL, 0, -1)
  gg.searchNumber("1.1F;1.4F:3::1", gg.TYPE_DOUBLE, false, gg.SIGN_EQUAL, 0, -1)
  gg.searchNumber("1.1D;1.4D:2::10", gg.TYPE_DOUBLE, false, gg.SIGN_EQUAL, 0, -1)
  gg.clearResults()
  gg.setRanges(gg.REGION_BAD)
  gg.searchNumber("2.718519e-43F;3.7615819e-37F;2.0F;0.00999999978F::200", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
  gg.clearResults()
  gg.searchNumber("2.718519e-43F;3.7615819e-37F;2.0F;0.00999999978F::200", gg.TYPE_FLOAT, false, gg.SIGN_EQUAL, 0, -1)
  gg.clearResults()
  gg.clearResults()
  gg.setRanges(gg.REGION_ANONYMOUS | gg.REGION_BAD)
  gg.searchNumber("1", gg.TYPE_QWORD | gg.TYPE_XOR | gg.TYPE_FLOAT | gg.TYPE_WORD | gg.TYPE_BYTE | gg.TYPE_DOUBLE | gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
  gg.setRanges(gg.REGION_ANONYMOUS | gg.REGION_BAD)
  gg.searchNumber("1", gg.TYPE_QWORD | gg.TYPE_XOR | gg.TYPE_FLOAT | gg.TYPE_WORD | gg.TYPE_BYTE | gg.TYPE_DOUBLE | gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1)
  gg.searchNumber("1.2F;1.8F:9::1", gg.TYPE_DOUBLE, false, gg.SIGN_EQUAL, 0, -1)
  gg.searchNumber("1.1F;1.4F:3::1", gg.TYPE_DOUBLE, false, gg.SIGN_EQUAL, 0, -1)
  gg.searchNumber("1.1D;1.4D:2::10", gg.TYPE_DOUBLE, false, gg.SIGN_EQUAL, 0, -1)
  gg.clearResults()
  gg.searchNumber("5001;1.1;1F::10", gg.TYPE_DOUBLE, false, gg.SIGN_EQUAL, 0, -1)
  gg.clearResults(850)
  gg.toast("ʙʏᴘᴀss ᴠ𝟸 ON")
end

function exitScript()
    gg.toast("Exiting Script...")
    os.exit()
end

while true do
    local ngay_gio = os.date("*t") -- Lấy ngày, giờ, phút, giây

    local mainMenu = gg.choice({
        "❌️ᴜɴsᴀғᴇ/ᴋʜôɴɢ ᴀɴ ᴛᴏàɴ",
        "✅️sᴀғᴇ/ᴀɴ ᴛᴏàɴ",
        "🛡 Bypass",
        "👋ʟᴇᴀᴠᴇ sᴄʀɪᴘᴛ/ᴛʜᴏáᴛ sᴄʀɪᴘᴛ"
    }, nil, "ⓜⓐⓓⓔ ⓑⓨ ⒹⒶⓋ\nHôm nay là: " ..
        string.format("%02d/%02d/%04d - %02d:%02d:%02d",
            ngay_gio.day, ngay_gio.month, ngay_gio.year,
            ngay_gio.hour, ngay_gio.min, ngay_gio.sec) ..
        "\n\n𝐒𝐞𝐥𝐞𝐜𝐭 𝐭𝐡𝐞 𝐒𝐚𝐟𝐞 𝐭𝐚𝐛/𝐜𝐡ọ𝐧 𝐭𝐚𝐛 𝐬𝐚𝐟𝐞 𝐧𝐡é")

    if mainMenu == 1 then
        local cheatMenu = gg.multiChoice({
            " ʙᴏᴅʏ ʜᴇᴀᴅsʜᴏᴛ",
            " ᴍᴀɢɪᴄ ʙᴜʟʟᴇᴛ",
            " ɴɪɢʜᴛ ᴍᴏᴅᴇ"
        }, nil, "𝐃𝐨 𝐧𝐨𝐭 𝐮𝐬𝐞 𝐢𝐭, 𝐨𝐫 𝐲𝐨𝐮 𝐰𝐢𝐥𝐥 𝐛𝐞 𝐩𝐞𝐫𝐦𝐚𝐧𝐞𝐧𝐭𝐥𝐲 𝐛𝐚𝐧𝐧𝐞𝐝 𝐟𝐫𝐨𝐦 𝐅𝐫𝐞𝐞 𝐅𝐢𝐫𝐞.")

        if cheatMenu then
            if cheatMenu[1] then bodyHeadshot() end
            if cheatMenu[2] then magicBullet() end
            if cheatMenu[3] then whiteBody() end
            if cheatMenu[4] then antinaHand() end
        end

    elseif mainMenu == 2 then
        local safeMenu = gg.multiChoice({
            " ᴀɪᴍ ᴀᴡᴍ (safe)",
            " ɪɴsᴛᴀɴᴛ ᴀᴡᴍ sᴡɪᴛᴄʜ (safe)",
            " ᴀɴᴛᴇɴɴᴀ ʜᴀɴᴅ ",
            " ᴀɴᴛᴇɴɴᴀ ʜᴇᴀᴅ (safe)"
        }, nil, "𝐔𝐬𝐞 𝐭𝐡𝐞𝐬𝐞 𝐟𝐮𝐧𝐜𝐭𝐢𝐨𝐧𝐬.")

        if safeMenu then
            if safeMenu[1] then awmAimbot() end
            if safeMenu[2] then awmFastSwitch() end
            if safeMenu[3] then antibanHead() end
        end

      elseif mainMenu == 3 then
        local bypassMenu = gg.multiChoice({
            "🛡 ʙʏᴘᴀss ᴠ𝟷",
            "🛡 ʙʏᴘᴀss ᴠ𝟸"
        }, nil, "𝐁𝐲𝐩𝐚𝐬𝐬 𝐀𝐧𝐭𝐢-𝐂𝐡𝐞𝐚𝐭 𝐟𝐨𝐫 𝐅𝐫𝐞𝐞 𝐅𝐢𝐫𝐞")

        if bypassMenu then
            if bypassMenu[1] then BYPASSPROV1() end
            if bypassMenu[2] then BYPASSPROV2() end
        end

    elseif mainMenu == 4 then
        exitScript()
    end

    gg.sleep(500) 
end
